{
  "$schema": "./schema.json",
  "config": {
    "updateChannel": "canary",
    "fontSize": 13,
    "fontFamily": "MonoLisa",
    "fontWeight": "normal",
    "fontWeightBold": "bold",
    "lineHeight": 1,
    "letterSpacing": 0,
    "scrollback": 1000,
    "cursorShape": "BLOCK",
    "cursorBlink": true,
    "borderColor": "#333",
    "termCSS": "",
    "windowSize": [
      800,
      600
    ],
    "catppuccinTheme": "Mocha",
    "workingDirectory": "",
    "showHamburgerMenu": "",
    "showWindowControls": "",
    "padding": "0rem 1rem 0rem 1rem",
    "shell": HYPER_SHELL,
    "shellArgs": [
      "--login"
    ],
    "env": {},
    "bell": "SOUND",
    "bellSound": null,
    "bellSoundURL": null,
    "copyOnSelect": true,
    "defaultSSHApp": true,
    "quickEdit": false,
    "macOptionSelectionMode": "vertical",
    "webGLRenderer": false,
    "webLinksActivationKey": "",
    "disableLigatures": false,
    "disableAutoUpdates": false,
    "autoUpdatePlugins": true,
    "preserveCWD": true,
    "screenReaderMode": false,
    "imageSupport": true,
    "defaultProfile": "default",
    "profiles": [
      {
        "name": "default",
        "config": {}
      }
    ],
    "opacity": 0.95,
    "uiFontFamily": "Inter",
    "clearCommand": "clear",
    "activeTab": "ðŸš€"
  },
  "plugins": [
    "hyper-opacity",
    "hyper-init",
    "hyper-active-tab",
    "hyper-pane",
    "hyper-search",
    "hypurr#latest"
  ],
  "localPlugins": [],
  "keymaps": {
    "tab:new": HYPER_KEYMAP_NEW_TAB,
    "pane:close": HYPER_KEYMAP_CLOSE_TAB,
    "editor:paste": HYPER_KEYMAP_PASTE
  }
}
